######
#
MAKE=make
PATSOPT=$(PATSHOME)/bin/patsopt
#
######
#
all:: \
Blink_dats.ino
Blink_dats.ino: \
Blink.dats; $(PATSOPT) -o $@ --dynamic $<
#
all:: ; $(MAKE) -f Makefile
#
######
#
all2:: \
Blink2_dats.ino
Blink2_dats.ino: \
Blink2.dats; $(PATSOPT) -o $@ --dynamic $<
#
all2:: ; $(MAKE) -f Makefile
#
######
#
all3:: \
Blink3_dats.ino
Blink3_dats.ino: \
Blink3.dats; $(PATSOPT) -o $@ --dynamic $<
#
all3:: ; $(MAKE) -f Makefile
#
######
#
RMF=rm -f
#
######
#
clean:: ; $(RMF) *~
clean:: ; $(RMF) *_?ats.c
#
######
#
cleanall:: clean
#
cleanall:: ; \
$(MAKE) -f Makefile clean
#
cleanall:: ; $(RMF) *_dats.ino
#
###### end of [Makefile_ats] ######
